        <div id="conference" class="tab-pane fade active in">
          <%- partial("_partial/schedule-registration-opening") %>

            <% site.pages.each(function(page){ if(page.speaker === "Jane Prusakova") { %>
              <%- partial("_partial/schedule-speaker", {page: page}) %>
