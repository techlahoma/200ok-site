<!-- Header -->
<header id="fullwidth" class="homepage">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12">
        <!-- Heading Title Area -->
        <div class="header-text white text-center text-uppercase">
          <div class="row">
            <div class="col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3 col-lg-4 col-lg-offset-4">
              <div class="pddn-20-top pddn-20-btm" style='text-align:center;'>
                <img src="assets/media/logo/200ok-white.svg" class="img-responsive" style='margin: 10px auto 0 auto; height: 140px'/>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-sm-10 col-sm-offset-1">
              <p style='font-size:1.5em; text-transform:none;'>
                Connect with fellow web professionals and some of the best speakers in the country
                for a day of presentations about leading web tech topics, plus join in hacking spaces,
                break-outs, and group discussions.
              </p>
              <h3 class="mrgn-50-top white sm-title mrgn-50-btm"><span class="date">June 3rd, 2016 – Tulsa, Oklahoma</span></h3>
              <p>
                <a href="https://ti.to/techlahoma/200ok" class="btn btn-primary btn-lg">Buy Tickets</a>
              </p>
            </div>
          </div>
        </div>
        <!-- /.Heading Title Area -->
      </div>
    </div>
  </div>
  <!-- /.container -->
</header>
<!-- /.Header -->

<!-- Section Sponsors -->
<section id="sponsors">
  <div class="container">
    <!-- Title & Subtitle -->
    <div class="row">
      <div class="col-md-12">
        <div class="section-title">
          <h2 class="text-center"><span>Sponsors</span></h2>
          <p class="subtitle text-center">
            This event couldn't be done without our sponsors.
            <a href='/sponsorship-prospectus/'>Interested in sponsoring?</a>
          </p>
          <p class="subtitle text-center"><i class="fa fa-angle-down fa-lg"></i></p>
        </div>
      </div>
    </div>
    <!-- /.Title & Subtitle -->

    <!-- Sponsors List -->
    <div class="row sponsor-row text-center">
      <h2 class="sponsor-row-description">&lt;h1&gt;</h2>
      <a href='http://www.gkff.org/' target='_blank'>
        <img class="img-responsive center-block" src="assets/media/sponsors/gkff.png" width="368" height="150" alt="George Kaiser Family Foundation" />
      </a>
    </div>
    <div class="row sponsor-row text-center">
      <h2 class="sponsor-row-description">&lt;h2&gt;</h2>
      <div class="col-sm-6 col-sm-offset-6 col-xs-12 col-xs-offset-6 mrgn-10-top-btm">
        <a href='https://www.mozilla.org/en-US/' target='_blank'>
          <img class="img-responsive center-block" src="assets/media/sponsors/mozilla.png" width="368" height="150" alt="" />
        </a>
      </div>
      <div class="col-sm-6 col-sm-offset-6 col-xs-12 col-xs-offset-6 mrgn-10-top-btm">
        <a href='http://www.gitwitcreative.com/#home' target='_blank'>
          <img class="img-responsive center-block" src="assets/media/sponsors/gitwit.png" width="150" height="150" alt="" style='padding-top:3%;'/>
        </a>
      </div>
    </div>
    <div class="row sponsor-row text-center">
      <h2 class="sponsor-row-description">Partners</h2>
      <a href='http://www.36degreesnorth.co/' target='_blank'>
        <img class="img-responsive center-block" src="assets/media/sponsors/36degreesnorth.jpg" width="368" height="150" alt="" style='padding-top:3%;'/>
      </a>
    </div>
    <!-- /.Sponsors List -->

    <!-- Local Sponsors -->
    <div class="row">
      <div class="col-md-12">
        <div class="section-title">
          <h3 class="text-center"><span>Local Sponsors</span></h3>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-sm-4 col-sm-offset-4 col-xs-6 col-xs-offset-3 mrgn-10-top-btm">
      </div>
      <div class="col-sm-4 col-sm-offset-4 col-xs-6 col-xs-offset-3 mrgn-10-top-btm">
        <a href='https://ithemes.com/' target='_blank'>
          <img class="img-responsive center-block" src="assets/media/sponsors/ithemes.png" width="184" height="75" alt="" />
        </a>
      </div>
    </div>
    <!-- /Local Sponsors -->
  </div>
<!-- /.container -->
</section>
<!-- /.Section Sponsors -->

<!-- Section Schedule -->
<section id="schedule">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="section-title">
          <h2 class="text-center"><span>Schedule</span></h2>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-lg-12">

        <!-- Schedule  -->
        <div class="row">
          <div class="col-lg-10 col-lg-offset-1">
            <div class="tab-content">
              <div class="tab-pane fade active in" id="day1">

                <div class="row">
                  <div class="col-xs-4 col-sm-2 text-center">
                    <p class="text-big text-bold">8:00am</p>
                  </div>
                  <div class="col-xs-8 col-sm-10">
                    <p class="text-big text-bold text-uppercase">Registration</p>
                    <p>Get checked in, mingle, and grab a bite to eat.</p>
                  </div>
                </div>

                <hr class="border-light">

                <div class="row">
                  <div class="col-xs-4 col-sm-2 text-center">
                    <p class="text-big text-bold">8:40am</p>
                  </div>
                  <div class="col-xs-8 col-sm-10">
                    <p class="text-big text-bold text-uppercase">Opening Announcements</p>
                  </div>
                </div>

                <hr class="border-light">

                <% site.pages.each(function(page){ if(page.speaker === "Ryan Hoegg") { %>
                  <%- partial("_partial/schedule-speaker", {page: page}) %>
                <% }}) %>

                <% site.pages.each(function(page){ if(page.speaker === "Greg Tatum") { %>
                  <%- partial("_partial/schedule-speaker", {page: page}) %>
                <% }}) %>

                <% site.pages.each(function(page){ if(page.speaker === "Jane Prusakova") { %>
                  <%- partial("_partial/schedule-speaker", {page: page}) %>
                <% }}) %>

                <!-- Lunch Break -->
                <div class="food-break white">
                  <div class="row">
                    <p>12:00pm-1:20pm<br>
                      Lunch Break</p>
                  </div>
                </div>
                <!-- /.Lunch Break -->
                <hr class="border-light">

                <div class="row">
                  <div class="col-xs-4 col-sm-2 text-center">
                    <p class="text-big text-bold">1:20pm-2:10pm</p>
                  </div>
                  <div class="col-xs-8 col-sm-10">
                    <p class="text-big text-bold text-uppercase">Lightning Talks</p>
                    <% site.pages.each(function(page){ if(page.lightning) { %>
                      <div style='overflow:hidden; border-top:1px solid #ccc; padding:1em 0'>
                        <img src="<%- page.photo %>"
                             alt="Speaker"
                             class="img-responsive img-circle img-speaker"
                             style="float:left; margin: 0.1em 1em;">
                          <p class="text-bold"><a href="<%- page.path %>"><%- page.talk %></a></p>
                        <p style="font-size:90%;"><%- page.speaker %></p>
                      </div>
                    <% }}) %>
                  </div>
                </div>

                <hr class="border-light">

                <% site.pages.each(function(page){ if(page.speaker === "Rob Carr") { %>
                  <%- partial("_partial/schedule-speaker", {page: page}) %>
                <% }}) %>

                <% site.pages.each(function(page){ if(page.speaker === "Greg Avola") { %>
                  <%- partial("_partial/schedule-speaker", {page: page}) %>
                <% }}) %>

                <div class="row">
                  <div class="col-xs-4 col-sm-2 text-center">
                    <p class="text-big text-bold">4:20pm</p>
                  </div>
                  <div class="col-xs-8 col-sm-10">
                    <p class="text-big text-bold text-uppercase">Closing Remarks</p>
                  </div>
                </div>

                <hr class="border-light">

                <div class="row">
                  <div class="col-xs-4 col-sm-2 text-center">
                    <p class="text-big text-bold">4:30pm-5:00pm</p>
                  </div>
                  <div class="col-xs-8 col-sm-10">
                    <p class="text-big text-bold text-uppercase">Socializing/Networking</p>
                  </div>
                </div>

                <hr class="border-light">

              </div>

            </div>
          </div>
        </div>
        <!-- /. Schedule  -->
      </div>
    </div>
  </div>
</section>
<section id="speakers">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="section-title">
          <h2 class="text-center"><span>Speakers</span></h2>
        </div>
      </div>
    </div>

    <!-- Speakers List -->
    <div class="row">
      <% site.pages.each(function(page){ if(page.speaker) { %>
        <div class="col-md-3 col-sm-6 text-center">
          <div class="speaker-item"> <a href="<%- page.path %>">
            <figure class="speaker accent-bg">
              <img src="<%- page.photo %>" alt="Speaker" class="img-responsive center-block" height="300" width="300">
              <figcaption>
                <p class="text-uppercase accent"><%- page.talk %></p>
                <p><%- page.time %></p>
              </figcaption>
            </figure>
            </a>
            <p>
              <span class="speaker-accent"><%- page.speaker %>&nbsp;</span><br>
              <span class="speaker-position"><%- page.position %>&nbsp;</span><br>
              <span class="speaker-company"><%- page.company %>&nbsp;</span>
            </p>
          </div>
        </div>
      <% }}) %>
    </div>
    <!-- /.Speakers List -->
</div>
<!-- /.container -->
</section>
<!-- /.Section Speakers -->

<!-- Section Location -->
<section id="location">
  <address>
    <h3 style='margin:0;'>
      <a href='http://www.36degreesnorth.co/' target='_blank'>Location: 36ºN</a>
    </h3>
    36 East Cameron Street<br/>
    Tulsa, OK 74103
  </address>
  <div id="google-container"></div>
  <div id="cd-zoom-in"></div>
  <div id="cd-zoom-out"></div>
</section>
<!-- /.Section Location -->

<section id="tickets">
  <p class="text-center">
    <a href="https://ti.to/techlahoma/200ok" class="btn btn-primary btn-lg">Buy Tickets</a>
  </p>
</section>
