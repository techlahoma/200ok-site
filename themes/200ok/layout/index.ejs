            <% site.pages.each(function(page){ if(page.speaker === "Vance Lucas") { %>
              <%- partial("_partial/schedule-speaker", {page: page}) %>
            <% }}) %>

            <% site.pages.each(function(page){ if(page.speaker === "Kassandra Perch") { %>
              <%- partial("_partial/schedule-speaker", {page: page}) %>
            <% }}) %>

